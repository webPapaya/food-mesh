<div id="dashboard">
  <h1>Admin Dashboard</h1>
  <% unless notice.nil? %>
    <p id="notice"><%= notice %></p>
  <%end%>

  <p> Logged in as: <%= current_user.email %>. <%= link_to "log out ", logout_path %>  </p>

  <%= render template: 'users/index'%>

  <h2> Administer </h2>
  <p> <%= link_to "daily intakes", daily_intakes_path%> </p>
  <p> <%= link_to "basic pages", basic_pages_path %> </p>
  <p> <%= link_to "clear search cache", clear_search_cache_path, :confirm => 'Do you really want to clear the search cache?' %> </p>
</div>
