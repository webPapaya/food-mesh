(function($, undefined){
    $("#header-search").submit(function(evt) {
        var main = $("#main-inner-wrp");
        var loader = $('<%= image_tag "loader.gif" %>');

        main.html(loader);
    });

    $("#main-menu").Tabs({
        wrapper: '#header-dropdown-wrp'
    });

    $('#pie-chart').PieChart({
        controll: $('#pie-chart-controlles')
    });


    $('#line-chart').LineChart();

})(jQuery);


